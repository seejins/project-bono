# Backend Environment Variables
# Copy this file to .env for development or set in production

# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration
# Leave empty for development (uses SQLite in-memory)
# Set to PostgreSQL connection string for production (Supabase)
# DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# Frontend URL for CORS
FRONTEND_URL=http://localhost:5173

# UDP Telemetry Configuration
DISABLE_UDP=false

# API Key for local host app communication
API_KEY=your-secret-api-key-here

# Admin Panel Password
ADMIN_PASSWORD=benji1234

# Session Configuration
SESSION_SECRET=your-session-secret-here