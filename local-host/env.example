# Local Host Environment Variables
# Copy this file to .env and configure for your setup

# Cloud API Configuration
# Replace with your actual Render app URL
CLOUD_API_URL=https://your-app.onrender.com

# API Key for authentication (must match backend API_KEY)
API_KEY=your-secret-api-key-here

# Season ID (optional, defaults to 'default-season')
SEASON_ID=default-season

# Debug mode (optional)
DEBUG=false
